{
  "version": 2,
  "routes": [
    {
      "src": "/assets/.*",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    { "src": "/.*", "dest": "/index.html" }
  ]
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://fi-rise.vercel.app/:path*",
      "permanent": true
    }
  ]
}
